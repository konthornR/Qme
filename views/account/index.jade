extend ../layout.jade
block append content
    .container
      .row
        .col-md-6.col-md-offset-3(style='text-align: center; margin-top: 20px;')
          img#logo-image.logo-header(src='../images/logo.jpg', alt='Logo')
      .row(style='margin-top: 40px;')
        .col-md-4.col-md-offset-4.col-sm-6.col-sm-offset-3(style='text-align: center;')
          a.btn.btn-lg.btn-theme(style='width: 100%;', href='/backstore/dashboard') Dashboard
      hr
      .row(style='margin-top: 30px;')
        .col-md-4.col-md-offset-4.col-sm-6.col-sm-offset-3
          .frame-page
            company-dropdown
            a.btn.btn-lg.btn-theme(style='display: inherit; margin-top: 30px; margin-bottom: 15px;', ng-href='../reserveQueue?companyId={{selectedCompany.id}}') จองคิว
            a.btn.btn-lg.btn-theme(style='display: inherit; margin-bottom: 15px;', ng-href='../queueLists?companyId={{selectedCompany.id}}') ตารางคิว
            a.btn.btn-lg.btn-theme(style='display: inherit; margin-bottom: 15px;', ng-href='../callQueue?companyId={{selectedCompany.id}}') เรียกคิว